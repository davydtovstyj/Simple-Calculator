<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <RootNamespace>Simple_Calculator</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Assets\icons8-calculator-50.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Rectangle 1.png" />
    <None Remove="Rectangle.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Assets\icons8-calculator-50.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Rectangle.png" />
  </ItemGroup>

</Project>
